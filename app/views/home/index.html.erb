<h3>Current User: </h3>
<% if user_signed_in? %>
  Email: <%= current_user.email %><br>
  Username: <%= current_user.username %><br>
  Firstname: <%= current_user.firstname %>

  emails: 
  <% if @emails.nil? %>
    "No emails!"
  <% else %>
    <% if @emails.count == 1 %>
      <%= @emails.data %>
    <% else %>
      <% @emails.each do |e| %>
        <p>Email from: <%=e.from_user%><br><%= e.data %></p>
      <% end %>
    <% end %>
  <% end %>

<% end %>
