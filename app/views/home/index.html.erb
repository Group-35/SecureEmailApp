<% if user_signed_in? %>
  Email: <%= current_user.email %>
  Username: <%= current_user.username %>
  Firstname: <%= current_user.firstname %>

  emails: 
  <% if @emails.nil? %>
    "No emails!"
  <% else %>
    <% if @emails.count == 1 %>
      <%= @emails.data %>
    <% else %>
      <% @emails.each do |e| %>

        <p><%= e.data %></p>
      
      <% end %>
    <% end %>
  <% end %>

<% end %>